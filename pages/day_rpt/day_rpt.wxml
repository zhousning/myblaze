    <!--处理水量-->
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">处理水量</label>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">进水量(吨)</label>
          <text class="weui-form-preview__value">{{inflow}}</text>
        </view>
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">出水量(吨)</label>
          <text class="weui-form-preview__value">{{outflow}}</text>
        </view>
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">耗电量(度)</label>
          <text class="weui-form-preview__value">{{power}}</text>
        </view>
      </view>
    </view>

    <!--污泥量-->
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">污泥量</label>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">进泥量(吨)</label>
          <text class="weui-form-preview__value">{{inmud}}</text>
        </view>
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">出泥量(吨)</label>
          <text class="weui-form-preview__value">{{outmud}}</text>
        </view>
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">污泥含水率(%)</label>
          <text class="weui-form-preview__value">{{mst}}</text>
        </view>
      </view>
    </view>

    <!--中水量-->
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">中水量</label>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">中水量(吨)</label>
          <text class="weui-form-preview__value">{{md}}</text>
        </view>
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">中水回用量(吨)</label>
          <text class="weui-form-preview__value">{{mdrcy}}</text>
        </view>
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">中水销售水量(吨)</label>
          <text class="weui-form-preview__value">{{mdsell}}</text>
        </view>
      </view>
