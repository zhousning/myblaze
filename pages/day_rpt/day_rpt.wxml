          <!-- <em class="weui-form-preview__value">¥2400.00</em> -->
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-flex">
          <view class="weui-flex__item">
            <view class="text-center"></view>
          </view>
          <view class="weui-flex__item">
            <view class="text-center">单价(元/吨)</view>
          </view>
          <view class="weui-flex__item">
            <view class="text-center">药剂浓度(%)</view>
          </view>
          <view class="weui-flex__item">
            <view class="text-center">投加量(吨)</view>
          </view>
        </view>
        <block wx:for="{{chemicals}}">
          <view class="weui-flex">
            <view class="weui-flex__item">
              <view class="text-center">{{item.name}}</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">{{item.unprice}}</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">{{item.cmptc}}</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">{{item.dosage}}</view>
            </view>
          </view>
        </block>
      </view>
    </view>

    <!--污泥处置-->
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">污泥处置</label>
        </view>
      </view>
      <block wx:for="{{tspmuds}}">
        <view class="weui-form-preview__bd">
          <view class="weui-flex">
            {{item.name}}
          </view>
          <view class="weui-flex">
            <view class="weui-flex__item">
              <view class="text-center">转运量</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">接收量</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">价格(元/吨)</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">处置</view>
            </view>
          </view>
          <view class="weui-flex">
            <view class="weui-flex__item">
              <view class="text-center">{{item.tspvum}}</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">{{item.rcpvum}}</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">{{item.price}}</view>
            </view>
            <view class="weui-flex__item">
              <view class="text-center">{{item.prtmtd}}</view>
            </view>
          </view>
        </view>
      </block>
    </view>

    <!--备注-->
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <label class="weui-form-preview__label">备注</label>
          <!-- <em class="weui-form-preview__value">¥2400.00</em> -->
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-flex">
          {{desc}}
        </view>
      </view>
    </view>

    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <block wx:if="{{state == '0'}}">
          <view class="text-primary">填报中</view>
        </block>
        <block wx:if="{{state == '1'}}">
          <block wx:if="{{jd == 0}}">
            <a class="weui-btn weui-btn_primary" bindtap="cmpVerifying">提交公司审核</a>
            <a class="weui-btn weui-btn_warn" bindtap="rejected">驳 回</a>
          </block>
          <view class="text-primary" wx:else>
            审核员审核中
          </view>
        </block>
        <view class="text-danger" wx:elif="{{state == '2'}}">审核员驳回</view>
        <block wx:elif="{{state == '3'}}">
          <block wx:if="{{jd == 1}}">
            <a class="weui-btn weui-btn_primary" bindtap="upreport" >上报水务集团</a>
            <a class="weui-btn weui-btn_warn" bindtap="rejected">驳 回</a>
          </block>
          <view class="text-info" wx:else>
            公司审核中
          </view>
        </block>
        <view class="text-danger" wx:elif="{{state == '4'}}">公司驳回</view>
        <view class="text-success" wx:elif="{{state == '5'}}">
          已上报水务集团
        </view>
      </view>
    </view>
  </view>
</view>